@model IEnumerable<SeguimientoEleccion.Web.Models.Elector>

@{
    ViewBag.Title = "Ver Punteados";
    Layout = "~/Views/Shared/_LayoutNoCache.cshtml";
}

<div class="page-header">
    <h4>Punteados</h4>
    <h3>@ViewBag.Colegio</h3>
</div>

<table class="table table-condensed table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DNI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Referente)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DNI)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Referente)
        </td>
    </tr>
}

</table>
